(FN main ()
    ($println("Hello, World!"))
)

(FN collatz (n)
    (IF (= n 1)
        ($println(1))
        (RET)
    )
    (IF (= % n 2 0)
        (LET x / n 2)
        ($println(x))
        ($collatz(x))
        (RET)
    )
    (LET x + * n 3 1)
    ($println(x))
    ($collatz(x))
)

(FN fib(n)
    (IF (< n 3)
        (RET n)
    )
    (RET + $fib(- n 1) $fib(- n 2))
)